   "source": [
    "def fibonacci_recursivo(n): \n",
    "    if n == 0: \n",
    "        return 0 \n",
    "    if n == 1: \n",
    "        return 1 \n",
    "    return fibonacci_recursivo(n-1) + fibonacci_recursivo(n-2) \n",
    "\n",
    "n = int(input('Enter your number n-simo: ')) \n",
    "fibonacci_recursivo(n+1)\n"
   ]
